{
  "expo": {
    "name": "PCC POS",
    "slug": "pilotProject",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Pilot Project on the Cafe Billing App.\n\nUser sends an text to a server and server calculates and returns the bill amount.",
    "githubUrl": "https://github.com/sibashisch/Cafe-app-react.git",
    "android": {
	    "package": "com.pccpos.pccpos"
    },
    "ios": {
	    "bundleIdentifier": "com.pccpos.pccpos"
    }
  }
}
